<h1 class="page-header">Edit Client</h1>
<%= render "form" %>

<h3 class="sub-header">Reset Client Password</h3>
<%= form_tag(reset_password_client_path, method: :patch, html: {role: "form"}) do %>
  <%= submit_tag "Reset", {type: "submit", class: "btn btn-default", data: { confirm: "Are you sure?" }} %>
<% end %>