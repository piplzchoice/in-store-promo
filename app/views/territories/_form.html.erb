<%= form_for @territory, html: {role: "form"} do |f| %>
  <div class="form-group">
    <%= f.label :name, {class: "control-label"} %>
    <%= f.text_field :name, {class: "form-control", placeholder: "Name"} %>
  </div>

  <%= f.button "Submit", {type: "submit", class: "btn btn-default"} %>
<% end %>